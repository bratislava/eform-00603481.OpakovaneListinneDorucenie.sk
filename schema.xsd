                  <xs:documentation xml:lang="sk">Odosielatel</xs:documentation>
                </xs:annotation>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Recipient" minOccurs="0" maxOccurs="unbounded" >
          <xs:complexType>
